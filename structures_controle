def signe(x):
    if x>0:
        print(x,'positif')
    elif x==0: #on vérifie cette condition si x n'est pas 
        #strictement positif
        print(x,'nul')
    else : 
        print(x,'négatif')

for elements in range(10,-10,-1): #début,fin,pas
    print(elements)

x=0
while x<10:
    print(x)
    x+=1

def fibonacci(n):
    list=[0,1]
    fi=0
    for i in range(2,n+1):
        list.append(list[i-1]+list[i-2])
    return(list)

